        <attr name="labelColor" />
        <attr name="valueColor" />
        <attr name="label" />
        <attr name="value" />
    </declare-styleable>

    <declare-styleable name="ItemIconView">
        <attr name="hasBottomLine" />
        <attr name="hasTopLine" />
        <attr name="hasDrawRightIcon" format="boolean" />
        <attr name="lineColor" />
        <attr name="labelColor" />
        <attr name="label" />
        <attr name="arrow" />
        <attr name="viewIcon" />
        <attr name="action" format="string" />
        <attr name="value" />
